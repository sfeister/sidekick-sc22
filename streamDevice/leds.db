record(stringin,"LEDS:info"){
    field (DESC, "Get information about the LED device")
	field(DTYP,"stream")
	field(INP,"@leds.proto info led_ino")
    field(PINI, "YES")
}

record(stringin,"LEDS:data"){
	field (DESC, "Get trigger and LED data")
	field (DTYP, "stream")
	field (SCAN, "I/O Intr")
	field (INP, "@leds.proto data leds_ino")
	field(PINI, "YES")
}

# Channel 0
record(longout,"LEDS:CH0:brig"){
    field (DESC, "Set LED brightness")
	field(DTYP,"stream")
	field(DRVL,"0")
	field(DRVH,"255")  
	field(OUT,"@leds.proto setBrightness(0) led_ino")
}

record(int64out,"LEDS:CH0:dur"){
    field(DESC, "Set LED duration (output)")
	field(DTYP,"stream")
	field(DRVL,"0")
	field(DRVH,"10000000") # restrict users from setting overly high times
	field(EGU,"micros")
	field(OUT,"@leds.proto setDuration(0) led_ino")
}

record(ao,"LEDS:CH0:red"){
    field (DESC, "Set LED red value")
	field(DTYP,"stream")
	field(DRVL,"0")
	field(DRVH,"1")  
	field(OUT,"@leds.proto setRed(0) led_ino")
}

record(ao,"LEDS:CH0:green"){
    field (DESC, "Set LED green value")
	field(DTYP,"stream")
	field(DRVL,"0")
	field(DRVH,"1")  
	field(OUT,"@leds.proto setGreen(0) led_ino")
}

record(ao,"LEDS:CH0:blue"){
